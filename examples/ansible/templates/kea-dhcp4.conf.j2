{
"Dhcp4": {
    "valid-lifetime": {{ valid_lifetime }},
    "renew-timer": {{ renew_timer }},
    "rebind-timer": {{ rebind_timer }},
    "interfaces-config": {
        "interfaces": [ "{{ interface_name }}" ]
    },
    "lease-database": {
        "type": "{{ lease_database_type }}",
        "persist": {{ lease_database_persist }},
        "name": "{{ lease_database_name }}"
    },
    "subnet4": [
        {
            "id": {{ subnet_id }},
            "subnet": "{{ subnet }}",
            "pools": [
                {
                    "pool": "{{ pool_range }}"
                }
            ]
        }
    ]
}
}
